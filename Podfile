# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'TechHuddle' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TechHuddle
  pod 'RxSwift'
  pod 'RxCocoa'

end

target 'TechHuddleTests' do
    use_frameworks!

    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RxTest'
    pod 'RxBlocking'
    
end
